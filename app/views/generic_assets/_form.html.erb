<%= simple_form_for(@generic_asset) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :main_title %>
    <%= f.input :alt_title %>
    <%= f.input :type %>
    <%= f.input :subjects %>
    <%= f.input :creator %>
    <%= f.input :photographer %>
    <%= f.input :author %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
